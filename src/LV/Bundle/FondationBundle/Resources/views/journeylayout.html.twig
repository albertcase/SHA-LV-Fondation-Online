{% extends '::base.html.twig' %}
{% block title %}筑梦之旅{% endblock %}
{% block head_style %}
{% stylesheets filter='cssrewrite' output="css/dream.css"
	'%kernel.root_dir%/Resources/public/css/reset.css'
	'%kernel.root_dir%/Resources/public/css/menu.css'
	'bundles/lvfondation/css/swiper.min.css'
	'bundles/lvfondation/css/style-dream.css'
%}
<link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
{% endstylesheets %}
{% endblock %}
{% block head_scripts %}
{% javascripts output="js/journey.js"
    '%kernel.root_dir%/Resources/public/js/*'
    '@LVFondationBundle/Resources/public/js/swiper.min.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
{% endblock %} 